@{
    ViewData["Title"] = "Home";
}
@model List<string>

<div class="row">
    <div class="col-md-12">
        <h2>Examples</h2>
        <div class="row">
            <div class="col-md-12">
                <ul>
                    @foreach (var item in @Model)
                    {
                        <li>
                            @Html.ActionLink(@item, "Video", "Home", new { id = @item },new { target = "_blank" })
                        </li>
                    }
                </ul>    
            </div>
        </div>
    </div>
</div>
